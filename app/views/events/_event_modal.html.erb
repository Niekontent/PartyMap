<div id="event-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <!-- Modal Content: begins -->
    <div class="modal-content">
      <!-- Modal Header -->
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
<!--        <h4 class="modal-title" id="gridSystemModalLabel">Your Headings</h4>-->
      </div>
      <!-- Modal Body -->
      <div class="modal-body">

        <div class="event-tile">
          <div id="modal-type" class="event-tile-type"> Typ </div>

          <div id="modal-photo" class="event-photo">
            <div class="event-photo-hover"><i class="fas fa-search"></i> Szczegóły </div>
          </div>
          <div class="event-info-wrapper">
            <div id='title' class="event-tile-title"> title </div>
            <div class="event-tile-details">
              <div id="date" class="event-tile-header-date chip">
                <i class="far fa-calendar-alt"></i> date
              </div>
              <div id="location" class="event-tile-header-location chip">
                <i class="fas fa-map-marker-alt"></i> location
              </div>
            </div>
          </div>

<!--          <div class="event-tile-description">-->
            <%#= event.description %>
<!--            <br>-->
<!--          </div>-->
<!--          <div class="event-action-buttons">-->
            <%# if current_user %>
<!--              <a class="btn <%#= (EventUser.user_has_event?(current_user, event) || Event.belongs_to_current_user?(current_user, event)) ? 'btn-secondary disabled' : 'btn-primary' %>" aria-disabled="true" href=" <%#= user_events_new_path(event.id) %> "><i class="fas fa-check"></i> Wchodzę w to!</a>-->
<!--              <a class="btn" href="#"><i class="fas fa-times"></i> Odpuszczam</a>-->
            <%# end %>
<!--          </div>-->
        </div>

        <div id="modal-title">Dupa</div>


      </div>




      <!-- Modal Footer -->
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
      </div>
    </div>
    <!-- Modal Content: ends -->
  </div>
</div>